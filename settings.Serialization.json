{
	"executeUnitTest": 0,
	"version": {
		"VersionFile": "./version.Serialization.json",
		"AssemblyInfoFile": "./src/Serialization/AssemblyInfo.Serialization.cs",
		"LoadFrom": "versionfile",
		"AutoIncrementVersion": false
	},
	"build": {
		"SourcePath": "./src",
		"SolutionFileSpec": "./src/XLabs.Serialization.sln",
		"TreatWarningsAsErrors": false,
		"EnableXamarinIOS": false,
		"MacAgentIPAddress": "",
		"MacAgentUserName": "",
		"MacAgentUserPassword": ""
	},
	"test": {
		"SourcePath": "./tests/Serialization",
		"ResultsPath": "./artifacts",
		"AssemblyFileSpec": "*.Tests.dll",
		"Framework": "NUnit3"
	},
	"nuget": {
		"NuGetConfig": "./.nuget/NuGet.config",
		"FeedUrl": "",
		"FeedAPIKey": "",
		"ArtifactsPath": "./artifacts/packages",
		"NuspecPath": "./nuspec/Serialization",
		"UpdateVersion": true,
		"UpdateLibraryDependencies": true,
		"VersionDependencyForLibrary": "greaterthanorequal",
		"LibraryNamespaceBase": "XLabs.Serialization",
		"LibraryMinVersionDependency": ""
	}
}
